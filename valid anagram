class Solution {
    public boolean isAnagram(String s, String t) {
        char[] c = s.toCharArray();
        char[] d = t.toCharArray();

        Arrays.sort(c);
        Arrays.sort(d);

        s = new String(c);
        t = new String(d);

        if(s.equals(t)){
            return true;
        }
        return false;
    }
}
